<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.task09sshvipsystem.vo.Commodity" table="commodity">
		<id name="commodityId" column="commodityid">
			<generator class="native"></generator>
		</id>
		<property name="commodityName" column="commodityname" type="string"></property>
		<property name="price" column="price" type="java.math.BigDecimal" scale="2"></property>
		<property name="agio" column="agio" type="java.math.BigDecimal" scale="3"></property>
	</class>
</hibernate-mapping>