<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.task09sshvipsystem.vo.Consume" table="consume">
		<id name="consumeId" column="consumeid">
			<generator class="native"></generator>
		</id>
		<property name="vipId" column="vipid" type="int"></property>
		<property name="name" column="name" type="string"></property>
		<property name="commodityId" column="commodityid" type="int"></property>
		<property name="commodityName" column="commodityname" type="string"></property>
		<property name="price" column="price" type="java.math.BigDecimal"></property>
		<property name="pracitcePrice" column="practiceprice" type="java.math.BigDecimal"></property>
	</class>
</hibernate-mapping>